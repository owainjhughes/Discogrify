{
  "version": 2,
  "buildCommand": "npm run vercel-build",
  "builds": [
    { "src": "dist/app.js", "use": "@vercel/node" },
    { "src": "dist/templates/**", "use": "@vercel/static" }
  ],
  "routes": [
    { "src": "/static/(.*)", "dest": "/dist/templates/static/$1" },
    { "src": "/login", "dest": "/dist/app.js" },
    { "src": "/callback", "dest": "/dist/app.js" },
    { "src": "/", "dest": "/dist/app.js" },
    { "src": "/artists", "dest": "/dist/app.js" },
    { "src": "/logout", "dest": "/dist/app.js" },
    { "src": "/(.*)", "dest": "/dist/app.js" }
  ]
}