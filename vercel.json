{
  "version": 2,
  "buildCommand": "npm run build",
  "functions": {
    "dist/app.js": {
      "runtime": "nodejs18.x"
    }
  },
  "routes": [
    { "src": "/static/(.*)", "dest": "/templates/static/$1" },
    { "src": "/login", "dest": "/dist/app.js" },
    { "src": "/callback", "dest": "/dist/app.js" },
    { "src": "/", "dest": "/templates/index.html" },
    { "src": "/artists", "dest": "/dist/app.js" },
    { "src": "/logout", "dest": "/dist/app.js" },
    { "src": "/(.*)", "dest": "/dist/app.js" }
  ]
}