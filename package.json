{
  "name": "discogrify",
  "version": "1.0.0",
  "description": "Discogrify - Discover and rate your Spotify albums with community ratings from Discogs",
  "main": "dist/app.js",
  "scripts": {
    "clean": "rm -rf dist",
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "node dist/app.js",
    "prebuild": "mkdir -p dist",
    "build": "tsc && cp -r templates dist/ && cp *.js dist/ 2>/dev/null || true",
    "dev": "npx ts-node app.ts",
    "vercel-build": "npm run prebuild && npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/owainjhughes/discogrify.git"
  },
  "author": "Owain Hughes",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/owainjhughes/discogrify/issues"
  },
  "homepage": "https://github.com/owainjhughes/discogrify#readme",
  "dependencies": {
    "@neondatabase/serverless": "^1.0.1",
    "@types/dotenv": "^6.1.1",
    "axios": "^1.3.6",
    "cheerio": "^1.0.0-rc.12",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^16.5.0",
    "ejs": "^3.1.9",
    "express": "^4.18.2",
    "express-session": "^1.18.1",
    "html": "^1.0.0",
    "json2csv": "^6.0.0-alpha.2",
    "node-fetch": "^2.6.1",
    "pg": "^8.16.3",
    "querystring": "^0.2.1",
    "request": "^2.88.2"
  },
  "devDependencies": {
    "@types/cookie-parser": "^1.4.8",
    "@types/cors": "^2.8.18",
    "@types/express": "^5.0.1",
    "@types/express-session": "^1.18.1",
    "@types/pg": "^8.15.5",
    "@types/request": "^2.48.12",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3"
  }
}
